{
    "name": "ssr-pwa",
    "version": "0.0.0",
    "description": "Example project from an angular.io guide.",
    "license": "MIT",
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "node postinstall-patch.js",
        "dev:ssr": "ng run ssr-pwa:serve-ssr",
        "serve:ssr": "node dist/server/main.js",
        "build:ssr": "ng build && ng run ssr-pwa:server:local",
        "build:ssr:qa": "ng build --configuration=qa --deploy-url=https://cdn.moglix.com/online/qa/pwa/bundles/ && ng run ssr-pwa:server:qa && npm run swupdate:url:qa",
        "build:ssr:vpn": "ng build --configuration=vpn --deploy-url=https://cdn.moglix.com/online/vpn/pwa/bundles/ && ng run ssr-pwa:server:vpn && npm run swupdate:url:vpn && npm run add:webengage:service-worker",
        "build:ssr:prod": "ng build --prod --deploy-url=https://cdn.moglix.com/online/prod/pwa/bundles/ && ng run ssr-pwa:server:production && npm run swupdate:url:prod && npm run add:webengage:service-worker",
        "build:ssr:prodab": "ng build --configuration=prodab --deploy-url=https://cdn.moglix.com/online/prod/pwa-ab/bundles/ && ng run ssr-pwa:server:prodab && npm run swupdate:url:prodab && npm run add:webengage:service-worker",
        "prerender": "ng run ssr-pwa:prerender",
        "swupdate:url:qa": "./node_modules/.bin/ngsw-config dist/browser ./ngsw-config.json  https://cdn.moglix.com/online/qa/pwa/bundles/",
        "swupdate:url:vpn": "./node_modules/.bin/ngsw-config dist/browser ./ngsw-config.json  https://cdn.moglix.com/online/vpn/pwa/bundles/",
        "swupdate:url:prod": "./node_modules/.bin/ngsw-config dist/browser ./ngsw-config.json  https://cdn.moglix.com/online/prod/pwa/bundles/",
        "swupdate:url:prodab": "./node_modules/.bin/ngsw-config dist/browser ./ngsw-config.json  https://cdn.moglix.com/online/prod/pwa-ab/bundles/",
        "cssupdate:config:qa": "cp ./src/app/scss/qa/_config.scss ./src/app/scss/_config.scss",
        "cssupdate:dev": "cp ./src/app/scss/_config.dev.scss ./src/app/scss/_config.scss",
        "add:webengage:service-worker": "concat dist/browser/ngsw-worker.js webengage.js -o dist/browser/ngsw-worker.js && cp webengage.js dist/browser/sw.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.0.1",
        "@angular/common": "~11.0.1",
        "@angular/compiler": "~11.0.1",
        "@angular/core": "~11.0.1",
        "@angular/forms": "~11.0.1",
        "@angular/platform-browser": "~11.0.1",
        "@angular/platform-browser-dynamic": "~11.0.1",
        "@angular/platform-server": "~11.0.1",
        "@angular/router": "~11.0.1",
        "@angular/service-worker": "~11.0.1",
        "@nguniversal/express-engine": "^11.0.0",
        "@ngx-meta/core": "^9.0.0",
        "angularx-social-login": "^3.5.7",
        "concat": "^1.0.3",
        "credit-card-type": "^9.1.0",
        "express": "^4.15.2",
        "net": "^1.0.2",
        "ng-lazyload-image": "^9.1.0",
        "ng2-cc-library": "^1.0.1",
        "ngx-pagination": "^3.2.1",
        "ngx-siema": "git+https://github.com/Yogender-moglix/ngx-siema.git",
        "ngx-socket-io": "^3.0.1",
        "ngx-webstorage": "^7.0.1",
        "pinch-zoom-js": "^2.3.4",
        "rxjs": "^6.6.7",
        "rxjs-compat": "^6.3.3",
        "tslib": "^2.0.0",
        "underscore": "^1.13.1",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "^11.1.1",
        "@angular-devkit/architect": ">= 0.900 < 0.1200",
        "@angular-devkit/build-angular": "~0.1100.2",
        "@angular/cli": "~11.0.2",
        "@angular/compiler-cli": "~11.0.1",
        "@nguniversal/builders": "^11.0.0",
        "@types/express": "^4.17.8",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "firebase-tools": "^8.0.0",
        "fuzzy": "^0.1.3",
        "inquirer": "^6.2.2",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "jasmine-core": "~3.6.0",
        "jasmine-marbles": "~0.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "open": "^7.0.3",
        "protractor": "~7.0.0",
        "rxjs-tslint": "^0.1.8",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.3"
    }
}
